{
  "name": "AgileZen Add Sub",
  "version": "0.5",
  "manifest_version": 2,
  "description": "Adds sub-story according to the requirements of Telenor MVNO",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["https://agilezen.com/api/v1/projects/*"],
  "content_scripts": [
    {
      "matches": ["*://agilezen.com/project/*"],
      "js": ["jquery-v1.8.2.min.js", "addSub.js"]
    }
  ]
}
